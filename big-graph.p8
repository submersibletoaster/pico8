-- housekeeping
local profile={}
function prof_in(label)
  local v=stat(1)
  profile[label]=v
end

function prof_out(label)
  local v=stat(1)
  assert(type(profile[label])=="number")
  profile[label]=v-profile[label]
end

function getprof()
  local t={}
  for k,v in pairs(profile) do
    t[k]=v
  end
  profile={}
  return t
  
end

