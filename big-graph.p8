  if p.dv[1]<0 then p.flp=false end
  
  
  if  p.dv[1]!=0 then
    p.anim = p.anim_walk
  end

  if p.dv[2]<0 then
    p.anim = p.anim_down
  end
  
  if p.dv[2]>0 then
    p.anim = p.anim_up
    p.astep=1
  end
   
  
  if btnp(5) then
    debug=debug==false and true or false
  end

end,
