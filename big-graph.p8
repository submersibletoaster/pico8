printh("caught by " .. e.col )
  local start={}
  for k,v in pairs(g) do
    if #v>0 then
      add(start,k)
    end
  end
  srand(stat(1)*5000+stat(85))
  local new_idx=start[1+flr(rnd(#start))]
  local new_cell=idx2cell(new_idx)
  local new_pos=cell2mxy(new_cell[1],new_cell[2])
  
