e.new=function(cx,cy,col,home,lurk)
  local t={cx=cx,cy=cy}
  local pos=cell2mxy(cx,cy)
  t.col=col or 14
  t.dv={0,0}
  t.mg=cell2idx(cx,cy)
  t.home=cell2idx(home[1],home[2])
  t.lurk=cell2idx(lurk[1],lurk[2])
  --cell2idx(cx,cy)
  
  t.mode="c"
  t.mtk=0
  t.mtm=300
  t.pos=pos
  t.tm=45
  t.tk=flr(rnd(t.tm))
  t.path={cell2idx(cx,cy)}
  t.state="init"
  setmetatable(t,e)
  t:seek()
  return t
end

